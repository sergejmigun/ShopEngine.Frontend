    "compileOnSave": true,
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": false,
        "target": "es6",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "allowJs": true,
        "outDir": "../ShopEngine.Frontend/Scripts_built/"
    },
    "include": [
        "Scripts"
    ]
}
